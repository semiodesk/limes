<Presentation PresentationClass="profile">
  <PageOptions Columns="3"/>
  <WindowName>Search</WindowName>
  <PageBackLinkName></PageBackLinkName>
  <PageBackLinkURL></PageBackLinkURL>
  <PageSubTitle></PageSubTitle>
  <PageDescription></PageDescription>
  <PanelList>
    <Panel Type="header"  />
    <Panel Type="page" />
    <Panel Type="active">
      <Module ID="MainMenu">
      </Module>
      <Module ID="Gadgets" >
        <ParamList>
          <Param Name="HTML">
            <div id="gadgets-tools-left" class="gadgets-gadget-parent"></div>
          </Param>
        </ParamList>
      </Module>
    </Panel>
    <Panel Type="left">
      <Module ID="LatestUpdates">
        <ParamList>
          <Param Name="Show">5</Param>
          <Param Name="SeeMore">True</Param>
        </ParamList>
      </Module>
    </Panel>
    <Panel Alias="people" Type="main">
		<Module ID="SearchEverything" />
		<Module ID="SearchPerson">
		</Module> 
    </Panel>
    <Panel Type="passive" >
      <Module ID="TopSearchPhrase">
        <ParamList>
          <Param Name="Description">Most viewed (today)</Param>
          <Param Name="TimePeriod">d</Param>
        </ParamList>
      </Module>
      <Module ID="TopSearchPhrase">
        <ParamList>
          <Param Name="Description">Most viewed (month)</Param>
          <Param Name="TimePeriod">m</Param>
        </ParamList>
      </Module>
    </Panel>
    <Panel type="footer" />
  </PanelList>
</Presentation>

